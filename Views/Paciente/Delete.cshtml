@model WebApplication1.Models.Paciente

<h1>Eliminar</h1>

<h3>Estas seguro de eliminar este Paciente?</h3>
<div>
    <h4>Paciente</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>
